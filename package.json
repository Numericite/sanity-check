{
	"name": "sanity-check",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build",
		"check": "biome check .",
		"check:unsafe": "biome check --write --unsafe .",
		"check:write": "biome check --write .",
		"dev": "next dev --turbo",
		"preview": "next build && next start",
		"start": "next start",
		"typecheck": "tsc --noEmit",
		"payload": "PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload",
		"seed:dev": "NODE_ENV=development PAYLOAD_CONFIG_PATH=./src/payload/payload.config.ts PAYLOAD_DROP_DATABASE=true payload run ./src/payload/seed/index.ts",
		"seed:prod": "NODE_ENV=prod PAYLOAD_CONFIG_PATH=./src/payload/payload.config.ts payload run ./src/payload/seed/index.ts"
	},
	"dependencies": {
		"@chakra-ui/react": "^3.22.0",
		"@emotion/react": "^11.14.0",
		"@payloadcms/db-postgres": "3.59.1",
		"@payloadcms/next": "3.59.1",
		"@payloadcms/richtext-lexical": "3.59.1",
		"@tanstack/react-query": "^5.69.0",
		"@trpc/client": "^11.0.0",
		"@trpc/next": "^11.0.0",
		"@trpc/react-query": "^11.0.0",
		"@trpc/server": "^11.0.0",
		"dotenv": "^17.2.3",
		"graphql": "^16.11.0",
		"next": "^15.2.3",
		"next-themes": "^0.4.6",
		"payload": "3.59.1",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-icons": "^5.5.0",
		"sharp": "^0.34.2",
		"superjson": "^2.2.1",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^20.14.10",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"typescript": "^5.8.2"
	},
	"ct3aMetadata": {
		"initVersion": "7.39.3"
	},
	"packageManager": "yarn@4.9.2"
}
